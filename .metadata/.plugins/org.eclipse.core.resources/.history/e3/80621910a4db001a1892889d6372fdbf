package arrays;

import java.util.Scanner;

public class Stringrev {
public static void main(String[] args) {
	Scanner sc= new Scanner(System.in);
	int n=sc.nextInt();
    while(n-->0){
    String str=sc.next();
    String word[]=str.split(".");
    String rev="";
    for(String w:word) {
    	StringBuilder sb=new StringBuilder();
    	sb.reverse();
    	rev+=sb.toString();
    }
    System.out.println(rev);
	}
}
}
